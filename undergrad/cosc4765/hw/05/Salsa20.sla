            <ITEXT FONTSIZE="10" CH="#define ROTL(a,b) (((a) &lt;&lt; (b)) | ((a) &gt;&gt; (32 - (b))))"/>
            <para/>
            <ITEXT FONTSIZE="10" CH="#define QR(a, b, c, d)(b ^= ROTL(a + d, 7), c ^= ROTL(b + a, 9), d ^= ROTL(c + b,13),"/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH=" a ^= ROTL(d + c,18))"/>
            <para/>
            <ITEXT FONTSIZE="10" CH="#define ROUNDS 20"/>
            <para/>
            <ITEXT FONTSIZE="10" CH="void salsa20_block(uint32_t out[16], uint32_t const in[16]) {"/>
            <para/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="int i;"/>
            <para/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="uint32_t x[16];"/>
            <para/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="for (i = 0; i &lt; 16; ++i) x[i] = in[i];"/>
            <para/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="for (i = 0; i &lt; ROUNDS; i += 2) {QR(x[ 0], x[ 4], x[ 8], x[12]); QR(x[ 5], x[ 9], x[13], x[ 1]); QR(x[10], x[14], x[ 2], x[ 6]); QR(x[15], x[ 3], x[ 7], x[11]); QR(x[ 0], x[ 1], x[ 2], x[ 3]); QR(x[ 5], x[ 6], x[ 7], x[ 4]); QR(x[10], x[11], x[ 8], x[ 9]); QR(x[15], x[12], x[13], x[14]); }"/>
            <para/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="for (i = 0; i &lt; 16; ++i)"/>
            <para/>
            <tab FONTSIZE="10"/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="out[i] = x[i] + in[i];"/>
            <para/>
            <ITEXT CH="}"/>
